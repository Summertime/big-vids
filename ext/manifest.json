{
    "applications": {
        "gecko": {
            "id": "bigvids@summerti.me",
            "strict_min_version": "60.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "manifest_version": 2,
    "name": "Big Vids",
    "options_ui": {
        "page": "options.html",
        "browser_style": true,
        "chrome_style": true
    },
    "version": "0.3",
    "permissions": [
        "https://www.youtube.com/*",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ]
}
